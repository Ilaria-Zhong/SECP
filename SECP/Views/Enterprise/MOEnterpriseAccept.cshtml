@model IEnumerable<SECP.Models.Products>
@{
    ViewBag.Title = "MOEnterpriseAccept";
    Layout = "~/Views/Enterprise.cshtml";
}

<link href="~/css/enterpriseMo.css" rel="stylesheet" />
<script type="text/javascript" src="~/js/jquery.js"></script>
@using (Html.BeginForm())
{
    <div class="MOenterprise-main2">
        <div class="enterpriseRe-tips">
        </div>
        <div class="acceptEnterprise">
            <div class="table-tittle">
                <h2>已发布产品</h2>
                <p>共 @ViewData["Q"]个</p>
            </div>
            <div class="table">

                <table width="100%" border="0" cellspacing="0" cellpadding="0">
                    <thead>
                        <tr>
                            <th>@ViewData["a"]</th>
                            <th>编号</th>
                            <th>主题</th>
                            <th>发布时间</th>
                            <th>价格</th>
                            <th>专利号</th>
                            <th>修改信息</th>
                            <th>取消</th>
                        </tr>
                    </thead>
                    <tbody>
                         @foreach (SECP.Models.Products k in Model)
                        {
                            <tr>
                                <td></td>
                                <td>@k.Products_Id</td>
                                <td><a href="CKEnterpriseproduct?Id=@k.Products_Id">@k.Products_Theme</a></td>
                                <td>@k.Products_Time</td>
                                <td>@k.Products_Money</td>
                                <td>@k.Products_Number</td>
                                <td><a href="MKproduct?Id=@k.Products_Id">修改</a></td>
                                <td><button class="cancelbutton"name='QX' type='submit' value="@k.Products_Id"></button></td>
                           </tr>
                        }
                    </tbody>
                </table>
                 <div class="paging">
                    @Html.Raw(HttpUtility.HtmlDecode(@Html.MikePager(Model as PagedList<SECP.Models.Products>)))
                </div>
            </div>

        </div>
    </div>
}

